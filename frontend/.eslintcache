ss PurchaseProduct extends Component {\n    render() {\n        const { product } = this.props;\n        return (\n            <div id={`item-${this.props.listID}`} className=\"purchase-item\">\n                <div className=\"flex \">\n                    <div className=\"cart-image-wrapper\">\n                        <img src={product.image} alt='' />\n                    </div>\n                    <div className=\"item-description\">\n                        <h1 className=\"item-title\">\n                            {product.name}\n                        </h1>\n                        <p>\n                            {product.description}\n                        </p>\n                    </div>\n                </div>\n                <div className=\"product-amount\">\n                    <div className=\"light-container\">\n                        Quantidade\n                    </div>\n                    <div className=\"\">\n                        <p className=\"info-text\">1{this.props.product.amount}</p>\n                    </div>\n                    <small>Unidades</small>\n                </div>\n                <div className=\"price-container\">\n                    <div className=\"\">\n                        Total:\n                    </div>\n                    <div className=\"price\">\n                        R$ {product.price / 100},00\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nclass Purchase extends Component {\n    state = {\n        collapsed: true\n    }\n\n    toggleCollapse(event, element_id) {\n        let el = document.querySelector(`#purchase-${element_id} .purchase-product-list`)\n        el.style.display = this.state.collapsed ? \"initial\" : \"none\";\n\n        this.setState({\n            collapsed: !this.state.collapsed\n        })\n    }\n\n    render() {\n        const { purchase } = this.props;\n        let img_src = this.state.collapsed ? \"/images/expand.svg\" : \"/