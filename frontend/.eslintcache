ge } from './Components/ProfilePage';\nimport { ShoppingCartPage } from './Components/ShoppingCartPage';\nimport { CreateProductPage } from './Components/Admin/CreateProductPage';\n\nimport { BrowserRouter, Switch, Route } from 'react-router-dom';\nimport { Component } from 'react';\n\n\nlet local_cart = JSON.parse(localStorage.getItem('cart'));\nlet local_cart_amount = localStorage.getItem('cart_amount');\n\nconsole.log(local_cart);\n\nclass App extends Component {\n  state = {\n    logged_in: false,\n    cart_items: local_cart ? local_cart : [],\n    cart_amount: local_cart_amount ? Number(local_cart_amount) : 0,\n  }\n\n  setState(dict) {\n    super.setState(dict, () => {\n      localStorage.setItem('cart', JSON.stringify(this.state.cart_items))\n      localStorage.setItem('cart_amount', this.state.cart_amount)\n    });\n\n  }\n\n  addToCart(product) {\n    let hasID = this.state.cart_items.some(each => {\n      if (each.id == product.id) {\n        let newcart = [...this.state.cart_items];\n        let index = newcart.indexOf(each);\n        newcart[index].amount += 1\n        this.setState({\n          cart_items: [...newcart],\n          cart_amount: this.state.cart_amount + 1\n        })\n        return true\n      }\n      else {\n        return false\n      }\n    })\n\n    if (!hasID) {\n      this.setState({\n        cart_items: [...this.state.cart_items, product],\n        cart_amount: this.state.cart_amount + 1\n      })\n    }\n\n  }\n\n  render() {\n    const appState = [this.state, (dict) => this.setState(dict)]\n\n    return (\n      <BrowserRouter>\n        <div>\n          <Header appState={appState} />\n          <div style={{ minHeight: '200px' }}>\n            {/* <Route path=\"/\" exact component={Banner} /> */}\n\n            <Route path=\"/\" exact render={() => (\n              <Showcase\n                appState={appState}\n                addToCart={(product) => this.addToCart(product)} />\n            )} />\n\n            <Route path=\"/login\" exact render={props => (\n              <Login logged_in={this.state.logged_in}\n                setAppState={list => this.setState(list)} />\n            )} />\n\n            <Route path=\"/logout\" exact render={props => (\n              <Logout logged_in={this.state.logged_in}\n                setAppState={list => this.setState(list)} />\n            )} />\n\n            <Route path=\"/register\" exact component={Register} />\n            <Route path=\"/about\" exact component={About} />\n\n            <Route path=\"/product/:product_id\" exact appState={appState} render={(props) => (\n              <ProductDetail\n                props={props}\n                appState={appState}\n                addToCart={(product) => this.addToCart(product)} />\n            )} />\n            <Route path=\"/profile/:user_id\" exact component={ProfilePage} />\n\n            <Route path=\"/shopping-cart\" exact render={() => (\n              <ShoppingCartPage\n                addToCart={(product) => this.addToCart(product)}\n                appState={appState} />\n            )} />\n\n            <Route path=\"/create-product\" exact component={CreateProductPage} />\n\n          </div>\n          {/* <Showcase /> */}\n          <Footer />\n        </div>\n      </BrowserRouter>\n    );\n  }\n}\n\nexport default App;\n","/home/john/Coding/vix/src/Components/Banner.js",["193"],"import { Component } from \"react\";\n\n\nclass Banner extends Component {\n\n    render() {\n        return (\n            <div className=\"banner\">\n                <img src=\"/images/banner3.jpg\" />\n                <div className=\"text\">\n                    <h1> Big Text Inside Banner </h1>\n                    <p>\n                        {/* Lorem ipsum dolor sit amet consectetur \n                        adipisicing elit. Deleniti atque sapiente \n                        aut. Repellat aliquid offici */}\n                    </p>\n                </div>\n            </div>\n        )\n    }\n\n}\n\n\nexport { Banner }\n","/home/john/Coding/vix/src/Components/Footer.js",["194","195","196","197","198","199","200","201"],"import { Component } from \"react\"\n\nclass Footer extends Component {\n    render() {\n        return (\n            <footer>\n                <div className=\"useful-links\">\n                    <div className=\"links-column\">\n                        <h1>MORE INFO</h1>\n                        <a id =\"contact\" href=\"#\">Contact</a>\n                        <a href=\"#\">Home</a>\n                        <a href=\"#\">About</a>\n                        <a href=\"#\">FAQ</a>\n                    </div>\n                    <div className=\"links-column\">\n                        <h1>CATEGORIES</h1>\n                        <a href=\"#\">Lorem</a>\n                        <a href=\"#\">Ipsum</a>\n                        <a href=\"#\">Dolor</a>\n                        <a href=\"#\">Requiem</a>\n                    </div>\n\n                </div>\n                <div className=\"copyright\">\n                    © 2020 Vix Decoração . All Rights Reserved | Design by John\n                </div>\n            </footer >\n        )\n    }\n\n}\n\n\nexport { Footer }\n\n","/home/john/Coding/vix/src/Components/Header.js",[],"/home/john/Coding/vix/src/Components/Topbar.js",[],"/home/john/Coding/vix/src/Components/Navbar.js",["202","203","204","205"],"import { Component } from \"react\"\nimport { Link } from 'react-router-dom';\n\n\nconst NavbarItem = (props) => {\n    let highlighted = props.name === props.highlighted;\n    let cls = highlighted ? \"link navbar-item current-page\" : \"link navbar-item\"\n    return (\n        <Link to={props.to}\n            onClick={e => props.func(e, props.name)}\n            className={cls}>\n            {props.text}\n        </Link>\n    )\n}\n\n\nclass Navbar extends Component {\n    state = {\n        input_text: '',\n        highlighted: 'home'\n    }\n\n    handleChange(e) {\n        this.setState({\n            input_text: e.target.value\n        })\n    }\n\n    handleSubmit(e) {\n        e.preventDefault();\n        this.setState({\n            input_text: ''\n        })\n    }\n\n    changeCurrentPage(e, page) {\n        this.setState({\n            highlighted: page\n        })\n    }\n\n    showSearchBar(e) {\n        let element = document.querySelector('.search-bar input');\n        element.style.width = \"590px\";\n        element.style.padding = \"0 0 0 .6em\";\n\n    }\n\n    render() {\n        const [appState, setAppState] = this.props.appState\n        return (\n            <nav>\n                <div className=\"logo\">\n                    <Link to=\"/\">\n                        <img src=\"/images/vixlogo.png\" />\n                    </Link>\n                </div>\n                <div className=\"navbar-links\">\n                    <NavbarItem to=\"/\"\n                        func={(e, page) => this.changeCurrentPage(e, page)}\n                        name='home'\n                        highlighted={this.state.highlighted}\n                        text=\"Início\" />\n\n                    <NavbarItem to=\"/\"\n                        func={(e, page) => this.changeCurrentPage(e, page)}\n                        name='product'\n                        highlighted={this.state.highlighted}\n                        text=\"Produtos\" />\n\n                    <NavbarItem to=\"/about\"\n                        func={(e, page) => this.changeCurrentPage(e, page)}\n                        name='about'\n                        highlighted={this.state.highlighted}\n                        text=\"Sobre nós\" />\n\n                    <NavbarItem to=\"/contact\"\n                        func={(e, page) => this.changeCurrentPage(e, page)}\n                        name='contact'\n                        highlighted={this.state.highlighted}\n                        text=\"Contato\" />\n                </div>\n                <div className=\"search-bar-container\">\n                    <form className=\"search-bar\" onSubmit={e => this.handleSubmit(e)}>\n                        <input\n                            onChange={e => this.handleChange(e)}\n                            placeholder=\"O que você está procurando?\"\n                            value={this.state.input_text}>\n                        </input>\n                        <button type=\"button\" onClick={(e) => this.showSearchBar(e)}>\n                            <img className=\"icon\" src=\"/images/search-icon.svg\" />\n                        </button>\n                    </form>\n                </div>\n\n                <div className=\"shopping-cart\">\n                    <div className=\"cart-wrapper\">\n                        <Link to=\"/shopping-cart\">Meu carrinho</Link>\n                        <img src=\"/images/shopping-cart-icon.svg\" />\n                    </div>\n                    {appState.cart_items.length\n                        ?\n                        <small>{appState.cart_amount} Produtos</small>\n                        :\n                        <small> Carrinho Vazio </small>\n                    }\n                </div>\n            </nav>\n        )\n    }\n}\n\n\nexport { Navbar }\n",["206","207"],"/home/john/Coding/vix/src/Components/Showcase.js",["208"],"import { Component } from \"react\"\nimport { Link, Redirect } from \"react-router-dom\"\nimport { Product } from \"./Product\"\n\nclass Showcase extends Component {\n    state = {\n        results: null\n    }\n\n    async componentDidMount() {\n        let response = await fetch(\"http://127.0.0.1:8000/products/\", {\n            method: 'GET',\n            headers: {\n            },\n        })\n\n        let data = await response.json();\n        this.setState({\n            results: data.results\n        })\n        // console.log(data.results);\n\n    }\n\n    render() {\n        const {appState} = this.props\n        return (\n            <div className=\"showcase\">\n                <div className=\"title-container\">\n                    <h1 className=\"title\">Produtos</h1>\n                </div>\n                <div className=\"products\">\n                    {this.state.results\n                        ?\n                        this.state.results.reverse().map(product => {\n                            return (\n                                <Link to={`/product/${product.id}`}>\n                                    <Product name={product.name}\n                                        price={product.price / 100}\n                                        appState={appState}\n                                        addToCart={this.props.addToCart}\n                                        image={`${product.image}`} />\n                                </Link>\n                            )\n                        })\n                        :\n                        <div style={{height: '600px'}}></div>\n                    }\n                </div>\n            </div>\n        )\n    }\n\n}\n\n\nexport { Showcase }\n","/home/john/Coding/vix/src/Components/Product.js",["209","210"],"import { Component } from \"react\"\n\nclass Product extends Component {\n    addButton(e) {\n        const {...product} = this.props\n        this.props.addToCart(product)\n    }\n\n    render() {\n        return (\n            <div className=\"product\">\n                <div className=\"overlay\">\n                    {/* <h1>Produto {this.props.preço}</h1> */}\n                    <h1>Adicionar ao Carrinho</h1>\n                    <img src=\"images/shopping-cart-icon.svg\"/>\n                </div>\n                <div className=\"image-wrapper\">\n                    <img src={this.props.image} />\n                </div>\n                <div className=\"product-info\">\n                    <p className=\"product-title\">{this.props.name}</p>\n                    <p className=\"product-price price\">R$ {this.props.price},00</p>\n                </div>\n                {/* <button onClick={e => this.addButton(e)}>Adicionar</button> */}\n            </div>\n        )\n    }\n\n}\n\n\nexport { Product }\n","/home/john/Coding/vix/src/Components/Login.js",["211"],"import { Component } from \"react\"\nimport { Link, Redirect } from \"react-router-dom\"\n\nclass Login extends Component {\n    state = {\n        email: '',\n        password: '',\n        redirect: null\n    }\n\n    componentDidMount() {\n        if (this.props.logged_in) {\n            console.log(this.props.logged_in);\n            this.setState({\n                redirect: '/'\n            })\n        }\n        document.querySelector('.login-page').scrollIntoView({ behavior: 'smooth', block: 'center' });\n    }\n\n    handleSubmit(e) {\n        e.preventDefault();\n        fetch(\"http://127.0.0.1:8000/api-token-auth/\", {\n            method: 'POST',\n            // credentials: 'include',\n            headers: {\n                'Content-Type': 'application/x-www-form-urlencoded',\n                'Cookie': \"SameSite=None\",\n            },\n            // body: `csrfmiddlewaretoken=${cookie}&email=${this.state.email}&password=${this.state.password}`\n            // body: `csrfmiddlewaretoken=koDi8M7AEYaSCjXkp3NTJD3HGDx8DMg0yvtcXseuVKU1sQWr49Ket5JG1HDfip9a&email=${this.state.email}&password=${this.state.password}&sessionid=gmdz6848cgheuycexab5sho7lku6chro`\n            body: `username=${this.state.email}&password=${this.state.password}`\n        }).then(res => res.json().then(data => {\n            if (data.token) {\n                console.log(data.token);\n                localStorage.setItem('Token', data.token)\n                localStorage.setItem('username', data.username)\n                localStorage.setItem('user_id', data.user_id)\n                this.setState({\n                    redirect: '/'\n                })\n                this.props.setAppState({ logged_in: true })\n            }\n            else {\n                console.log('invalid credentials');\n            }\n            //window.location.replace('http://localhost:8000/login/');\n\n        }))\n    }\n\n\n    render() {\n        return (\n            this.state.redirect\n                ?\n                <Redirect to={this.state.redirect} />\n                :\n                <section className=\"login-page\"\n                    style={{\n                        backgroundImage: \"url(/images/about-background.jpg)\",\n                        backgroundSize: \"100%\",\n                        backgroundRepeat: \"no-repeat\",\n                        backgroundPosition: \"30%\",\n                    }}\n                >\n\n                    <div className=\"login-area\">\n                        <h1>Fazer Login</h1>\n                        <form onSubmit={e => this.handleSubmit(e)} method=\"POST\" action=\"http://127.0.0.1:8000/login/\">\n                            <input onChange={e => this.setState({ email: e.target.value })}\n                                name='email'\n                                placeholder=\"E-mail ou Usuário\"\n                                value={this.state.email}></input>\n                            <input onChange={e => this.setState({ password: e.target.value })}\n                                name='password'\n                                type='password'\n                                placeholder=\"Senha\"\n                                value={this.state.password}></input>\n                            <a href=\"#\">Esqueceu a senha?</a>\n                            <button type=\"submit\">Entrar</button>\n                            <Link to=\"/register\">Não é cadastrado? Cadastre-se aqui</Link>\n                        </form>\n                    </div>\n                </section>\n        )\n    }\n\n}\n\n\nexport { Login }\n","/home/john/Coding/vix/src/Components/Register.js",[],"/home/john/Coding/vix/src/Components/About.js",[],"/home/j