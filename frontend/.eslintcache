ta.amount = 1\n            this.setState({\n                product: data\n            })\n            document.querySelector('.image-container').scrollIntoView({ behavior: 'smooth', block: 'center' });\n        }\n        else {\n            this.setState({\n                product: null\n            })\n        }\n\n    }\n\n    render() {\n        return (\n            <div className=\"product-detail\">\n                {this.state.product\n                    ?\n                    <>\n                        <div className=\"image-container\">\n                            <img src={`${this.state.product.image}`}></img>\n                        </div>\n                        <div className=\"description\">\n                            <h1>{this.state.product.name}</h1>\n                            <p>{this.state.product.description}</p>\n                            <h2>R$ {this.state.product.price / 100},00</h2>\n                            <button onClick={e => this.addButton(e)}>Adicionar ao carrinho</button>\n                        </div>\n                    </>\n                    :\n                    <div style={{ height: \"600px\", width: \"600px\" }}>\n                    </div>\n                }\n            </div>\n        )\n    }\n}\n\nexport { ProductDetail }","/home/john/Coding/vix/src/Components/ProfilePage.js",[],"/home/john/Coding/vix/src/Components/ShoppingCartPage.js",["222","223","224"],"import { Component } from \"react\"\n\n\nclass CartItem extends Component {\n    constructor(props) {\n        super(props)\n        const { product } = props\n        this.state = {\n            decrease_button_disabled: product.amount === 1 ? true : false\n        }\n\n    }\n\n    removeProduct(e, item) {\n        const [appState, setAppState] = this.props.appState;\n\n        setAppState({\n            cart_items: [...appState.cart_items.filter((each) => each != item)],\n            cart_amount: appState.cart_amount - item.amount\n        })\n\n    }\n\n    decrease() {\n        const [appState, setAppState] = this.props.appState;\n        const { product } = this.props\n        if (product.amount === 1) return\n\n        if (product.amount === 2) {\n            this.setState({\n                decrease_button_disabled: true\n            })\n        }\n\n        const { cart_items } = appState\n\n        let newcart = [...cart_items];\n        let index = newcart.indexOf(product);\n        newcart[index].amount -= 1\n\n        setAppState({\n            cart_items: [...newcart],\n            cart_amount: appState.cart_amount - 1\n        })\n\n    }\n\n    increase() {\n        this.props.addToCart(this.props.product)\n        this.setState({\n            decrease_button_disabled: false\n        })\n    }\n\n    render() {\n        const { product } = this.props\n        return (\n            <div className=\"item flex\">\n                <div className=\"flex \">\n                    <div className=\"cart-image-wrapper\">\n                        <img src={product.image} alt='' />\n                        <button className=\"remove-product-button\"\n                            onClick={(e) => this.removeProduct(e, product)}>Remover Produto</button>\n                    </div>\n                    <div className=\"item-description\">\n                        <h1 className=\"item-title\">\n                            {product.name}\n                        </h1>\n                        <p>\n                            {product.description}\n                        </p>\n\n                        R$ {product.price / 100},00\n                    </div>\n                </div>\n                <div className=\"cart-controls-container\">\n                    <div className=\"light-container\">\n                        Quantidade\n                    </div>\n                    <div className=\"cart-item-controls flex\">\n                        {this.state.decrease_button_disabled\n                            ?\n                            <button className=\"decrease button button-disabled\" onClick={() => this.decrease()}>-</button>\n                            :\n                            <button className=\"decrease button control-button\" onClick={() => this.decrease()}>-</button>\n                        }\n                        <p className=\"info-text\">{this.props.product.amount}</p>\n                        <button className=\"increase control-button button\" onClick={() => this.increase()}>+</button>\n                    </div>\n                    <small>Unidades</small>\n                </div>\n                <div className=\"price-container\">\n                    <div className=\"light-container\">\n                        Total\n                    </div>\n                    <div className=\"price\">\n                        R$ {product.price * product.amount / 100},00\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nclass ShoppingCartPage extends Component {\n\n    emptyCart(e) {\n        const [appState, setAppState] = this.props.appState\n        setAppState({\n            cart_items: [],\n            cart_amount: 0\n        })\n\n    }\n\n    render() {\n        const [appState, setAppState] = this.props.appState\n        const cart_items = appState.cart_items\n\n        return (\n            <div className=\"shopping-cart-section\">\n                <h1 className=\"page-title\">Produtos adicionados ao carrinho</h1>\n                <div className=\"cart-items\" >\n                    {cart_items.map(item => {\n                        return (\n                            <CartItem\n                                addToCart={this.props.addToCart}\n                                product={item}\n                                appState={this.props.appState} />\n\n                        )\n                    })\n                    }\n\n                    <button onClick={e => this.emptyCart(e)}>Esvaziar Carrinho</button>\n                    <button>Finalizar Compra</button>\n                </div>\n            </div>\n        )\n    }\n\n}\n\n\nexport { ShoppingCartPage }\n","/home/john/Coding/vix/src/Components/Admin/CreateProductPage.js",["225","226"],"import { Component } from \"react\"\nimport { Link, Redirect } from \"react-router-dom\"\n\n\nclass CreateProductPage extends Component {\n    state = {\n        name: '',\n        description: '',\n        price: '',\n        image: null,\n    }\n\n    handleSubmit(e) {\n        const { name, description, price, image } = this.state\n        e.preventDefault();\n\n        let data = new FormData();\n        data.append('name', name);\n        data.append('description', description);\n        data.append('price', price);\n        data.append('image', image);\n\n        fetch(\"http://127.0.0.1:8000/products/\", {\n            method: 'POST',\n            headers: {\n                // 'Content-Type': 'multipart/form-data;boundary=&',\n                // 'Authorization': \"Token ef6ff0a33959d98fcb27c7efe82f349e8da13854\"\n            },\n            // body: `name=${name}&description=${description}&price=${price}&image=${image}`\n            body: data\n        }).then(res => res.json().then(data => {\n            console.log(data);\n            //window.location.replace('http://localhost:8000/login/');\n\n        }))\n    }\n\n    render() {\n        console.log(this.state);\n        return (\n            <div style={{ backgroundColor: \"#fff\", padding: \"5em 0\" }}>\n                <form onSubmit={(e) => this.handleSubmit(e)}>\n                    <div>\n                        <label>Nome</label>\n                        <input required\n                            onChange={e => this.setState({ name: e.target.value })}></input>\n                    </div>\n                    <div>\n                        <label>Descrição</label>\n                        <textarea onChange={e => this.setState({ description: e.target.value })}>\n                        </textarea>\n                    </div>\n                    <div>\n                        <label>Preço</label>\n                        <input type=\"number\"\n                            min='0'\n                            step='0.01'\n                            placeholder=\"Ex: 50.00\"\n                            required\n                            onChange={e => this.setState({ price: e.target.value })}></input>\n                    </div>\n                    <div>\n                        <label>Imagem</label>\n                        <input type=\"file\" accept=\"image/*\" required\n                            onChange={e => this.setState({ image: e.target.files[0] })}></input>\n                    </div>\n\n                    <button type=\"submit\">Criar</button>\n                </form >\n            </div >\n        )\n    }\n}\n\nexport { CreateProductPage }","/home/john/PycharmProjects/vix-backend/frontend/src/index.js",[],["227","228"],"/home/john/PycharmProjects/vix-backend/frontend/src/App.js",[],"/home/john/PycharmProjects/vix-backend/frontend/src/Components/Header.js",[],"/home/john/PycharmProjects/vix-backend/frontend/src/Components/Banner.js",["229"],["230","231"],"/home/john/PycharmProjects/vix-backend/frontend/src/Components/Showcase.js",["232","233"],"import { Component } from \"react\"\nimport { Link } from \"react-router-dom\"\nimport { Product } from \"./Product\"\n\nclass Showcase extends Component {\n    state = {\n        results: null\n    }\n\n    async componentDidMount() {\n        let response = await fetch(\"http://127.0.0.1:8000/products/\", {\n            method: 'GET',\n            headers: {\n            },\n        })\n\n        let data = await response.json();\n        this.setState({\n            results: data.results\n        })\n        // console.log(data.results);\n\n        document.querySelector('body').scrollIntoView({ behavior: 'smooth', block: 'start' });\n\n 