 => {\n      localStorage.setItem('cart', JSON.stringify(this.state.cart_items))\n      localStorage.setItem('cart_amount', this.state.cart_amount)\n    });\n\n  }\n\n  addToCart(product) {\n    let hasID = this.state.cart_items.some(each => {\n      if (each.id == product.id) {\n        let newcart = [...this.state.cart_items];\n        let index = newcart.indexOf(each);\n        newcart[index].amount += 1\n        this.setState({\n          cart_items: [...newcart],\n          cart_amount: this.state.cart_amount + 1\n        })\n        return true\n      }\n      else {\n        return false\n      }\n    })\n\n    if (!hasID) {\n      this.setState({\n        cart_items: [...this.state.cart_items, product],\n        cart_amount: this.state.cart_amount + 1\n      })\n    }\n\n  }\n\n  render() {\n    const appState = [this.state, (dict) => this.setState(dict)]\n\n    return (\n      <BrowserRouter>\n        <div>\n          <Header appState={appState} />\n          <div style={{ minHeight: '200px' }}>\n            {/* <Route path=\"/\" exact component={Banner} /> */}\n\n            <Route path=\"/\" exact render={() => (\n              <Showcase\n                appState={appState}\n                addToCart={(product) => this.addToCart(product)} />\n            )} />\n\n            <Route path=\"/login\" exact render={props => (\n              <Login logged_in={this.state.logged_in}\n                setAppState={list => this.setState(list)} />\n            )} />\n\n            <Route path=\"/logout\" exact render={props => (\n              <Logout logged_in={this.state.logged_in}\n                setAppState={list => this.setState(list)} />\n            )} />\n\n            <Route path=\"/register\" exact component={Register} />\n            <Route path=\"/about\" exact component={About} />\n\n            <Route path=\"/product/:product_id\" exact appState={appState} render={(props) => (\n              <ProductDetail\n                props={props}\n                appState={appState}\n                addToCart={(product) => this.addToCart(product)} />\n            )} />\n            <Route path=\"/profile/:user_id\" exact component={ProfilePage} />\n\n            <Route path=\"/shopping-cart\" exact render={() => (\n              <ShoppingCartPage\n                addToCart={(product) => this.addToCart(product)}\n                appState={appState} />\n            )} />\n\n            <Route path=\"/create-product\" exact component={CreateProductPage} />\n\n          </div>\n          {/* <Showcase /> */}\n          <Footer />\n        </div>\n      </BrowserRouter>\n    );\n  }\n}\n\nexport default App;\n","/home/john/Coding/vix/src/Components/Banner.js",["180"],"import { Component } from \"react\";\n\n\nclass Banner extends Component {\n\n    render() {\n        return (\n            <div className=\"banner\">\n                <img src=\"/images/banner3.jpg\" />\n                <div className=\"text\">\n                    <h1> Big Text Inside Banner </h1>\n                    <p>\n                        {/* Lorem ipsum dolor sit amet consectetur \n                        adipisicing elit. Deleniti atque sapiente \n                        aut. Repellat aliquid offici */}\n                    </p>\n                </div>\n            </div>\n        )\n    }\n\n}\n\n\nexport { Banner }\n","/home/john/Coding/vix/src/Components/Footer.js",["181","182","183","184","185","186","187","188"],"import { Component } from \"react\"\n\nclass Footer extends Component {\n    render() {\n        return (\n            <footer>\n                <div className=\"useful-links\">\n                    <div className=\"links-column\">\n                        <h1>MORE INFO</h1>\n                        <a id =\"contact\" href=\"#\">Contact</a>\n                        <a href=\"#\">Home</a>\n                        <a href=\"#\">About</a>\n                        <a href=\"#\">FAQ</a>\n                    </div>\n                    <div className=\"links-column\">\n                        <h1>CATEGORIES</h1>\n                        <a href=\"#\">Lorem</a>\n                        <a href=\"#\">Ipsum</a>\n                        <a href=\"#\">Dolor</a>\n                        <a href=\"#\">Requiem</a>\n                    </div>\n\n                </div>\n                <div className=\"copyright\">\n                    © 2020 Vix Decoração . All Rights Reserved | Design by John\n                </div>\n            </footer >\n        )\n    }\n\n}\n\n\nexport { Footer }\n\n","/home/john/Coding/vix/src/Components/Header.js",[],"/home/john/Coding/vix/src/Components/Topbar.js",[],"/home/john/Coding/vix/src/Components/Navbar.js",["189","190","191","192"],"import { Component } from \"react\"\nimport { Link } from 'react-router-dom';\n\n\nconst NavbarItem = (props) => {\n    let highlighted = props.name === props.highlighted;\n    let cls = highlighted ? \"link navbar-item current-page\" : \"link navbar-item\"\n    return (\n        <Link to={props.to}\n            onClick={e => props.func(e, props.name)}\n            className={cls}>\n            {props.text}\n        </Link>\n    )\n}\n\n\nclass Navbar extends Component {\n    state = {\n        input_text: '',\n        highlighted: 'home'\n    }\n\n    handleChange(e) {\n        this.setState({\n            input_text: e.target.value\n        })\n    }\n\n    handleSubmit(e) {\n        e.preventDefault();\n        this.setState({\n            input_text: ''\n        })\n    }\n\n    changeCurrentPage(e, page) {\n        this.setState({\n            highlighted: page\n        })\n    }\n\n    showSearchBar(e) {\n        let element = document.querySelector('.search-bar input');\n        element.style.width = \"590px\";\n        element.style.padding = \"0 0 0 .6em\";\n\n    }\n\n    render() {\n        const [appState, setAppState] = this.props.appState\n        return (\n            <nav>\n                <div className=\"logo\">\n                    <Link to=\"/\">\n                        <img src=\"/images/vixlogo.png\" />\n                    </Link>\n                </div>\n                <div className=\"navbar-links\">\n                    <NavbarItem to=\"/\"\n                        func={(e, page) => this.changeCurrentPage(e, page)}\n                        name='home'\n                        highlighted={this.state.highlighted}\n                        text=\"Início\" />\n\n                    <NavbarItem to=\"/\"\n                        func={(e, page) => this.changeCurrentPage(e, page)}\n                        name='product'\n                        highlighted={this.state.highlighted}\n                        text=\"Produtos\" />\n\n                    <NavbarItem to=\"/about\"\n                        func={(e, page) => this.changeCurrentPage(e, page)}\n                        name='about'\n                        highlighted={this.state.highlighted}\n                        text=\"Sobre nós\" />\n\n                    <NavbarItem to=\"/contact\"\n                        func={(e, page) => this.changeCurrentPage(e, page)}\n                        name='contact'\n                        highlighted={this.state.highlighted}\n                        text=\"Contato\" />\n                </div>\n                <div className=\"search-bar-container\">\n                    <form className=\"search-bar\" onSubmit={e => this.handleSubmit(e)}>\n                        <input\n                            onChange={e => this.handleChange(e)}\n                            placeholder=\"O que você está procurando?\"\n                            value={this.state.input_text}>\n                        </input>\n                        <button type=\"button\" onClick={(e) => this.showSearchBar(e)}>\n                            <img className=\"icon\" src=\"/images/search-icon.svg\" />\n                        </button>\n                    </form>\n                </div>\n\n                <div className=\"shopping-cart\">\n                    <div className=\"cart-wrapper\">\n                        <Link to=\"/shopping-cart\">Meu carrinho</Link>\n                        <img src=\"/images/shopping-cart-icon.svg\" />\n                    </div>\n                    {appState.cart_items.length\n                        ?\n                        <small>{appState.cart_amount} Produtos</small>\n                        :\n                        <small> Carrinho Vazio </small>\n                    }\n                </div>\n            </nav>\n        )\n    }\n}\n\n\nexport { Navbar }\n",["193","194"],"/home/john/Coding/vix/src/Components/Showcase.js",["195"],"import { Component } from \"react\"\nimport { Link, Redirect } from \"react-router-dom\"\nimport { Product } from \"./Product\"\n\nclass Showcase extends Component {\n    state = {\n        results: null\n    }\n\n    async componentDidMount() {\n        let response = await fetch(\"http://127.0.0.1:8000/products/\", {\n            method: 'GET',\n            headers: {\n            },\n        })\n\n        let data = await response.json();\n        this.setState({\n            results: data.results\n        })\n        // console.log(data.results);\n\n    }\n\n    render() {\n        const {appState} = this.props\n        return (\n            <div className=\"showcase\">\n                <div className=\"title-container\">\n                    <h1 className=\"title\">Produtos</h1>\n                </div>\n                <div className=\"products\">\n                    {this.state.results\n                        ?\n                        this.state.results.reverse().map(product => {\n                            return (\n                                <Link to={`/product/${product.id}`}>\n                                    <Product name={product.name}\n                                        price={product.price / 100}\n                                        appState={appState}\n                             