products);\n        if (res.status == 201) {\n            localStorage.removeItem('cart')\n            localStorage.removeItem('cart_amount')\n            this.setState({\n                purchase_confirmed: true\n            })\n\n            setAppState({\n                cart_items: [],\n                cart_amount: 0\n            })\n        }\n        else {\n            console.log('fail');\n        }\n\n\n    }\n\n    async componentDidMount() {\n        // let user_id = localStorage.getItem('user_id')\n        // let token = localStorage.getItem('Token')\n        // let URL = `http://localhost:8000/purchase_orders`\n        // let res = await fetch(URL, {\n        //     method: 'POST',\n        //     headers: {\n        //         'Authorization': `Token ${token}`\n        //     }\n        // })\n        // let data = await res.json();\n        // console.log(data.products);\n    }\n\n    render() {\n        return (\n            this.state.purchase_confirmed\n                ?\n                <div>\n                <h1>Compra realizada com sucesso</h1>\n                    <Link to=\"/my_purchases\">Ver minhas compras</Link>\n                </div>\n                :\n                <div>\n                    {/* <h1>Suas Compras</h1> */}\n                    <button className=\"main-button\" onClick={e => this.createPurchasingOrder(e)}>Confirmar Compra</button>\n                </div>\n        )\n    }\n}\n\nexport { PurchaseOrders }","/home/john/PycharmProjects/vix-backend/frontend/src/Components/MyPurchases.js",["254","255","256","257"],"/home/john/PycharmProjects/vix-backend/frontend/src/Components/ContactPage.js",[],{"ruleId":"258","replacedBy":"259"},{"ruleId":"260","replacedBy":"261"},{"ruleId":"262","severity":1,"message":"263","line":2,"column":10,"nodeType":"264","messageId":"265","endLine":2,"endColumn":16},{"ruleId":"262","severity":1,"message":"266","line":15,"column":25,"nodeType":"264","messageId":"265","endLine":15,"endColumn":31},{"rul